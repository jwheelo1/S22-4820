--- 	county_name			text,
--- 	state_code			text,
--- 	from_gdp			int,
--- 	to_gdp				int,
--- 	growth_amt			int,
--- 	growth_pct			float,		-- pct implies multiped by 100.0
--- 	from_year			int default 2010,
--- 	to_year				int default 2020
--- );
